# Copy this file to .env and fill in your secrets. Never commit your real .env.

# Google Gemini (google-generativeai)
GOOGLE_API_KEY=your_google_generativeai_api_key_here

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud DLP (usually uses service account; path to JSON key file)
# Example on Windows: C:\\path\\to\\service-account.json
# Example on macOS/Linux: /absolute/path/to/service-account.json
GOOGLE_APPLICATION_CREDENTIALS=/absolute/path/to/service-account.json

# AWS (used by boto3)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
